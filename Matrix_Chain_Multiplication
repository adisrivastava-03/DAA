#include<stdio.h>
#define inf 9999
void matrix_chain(int p[],int size){
	int i,l,k,q,j;
	int n=size-1;
	int m[n][n],s[n][n];
	for(int i=0;i<n;i++)
		m[i][i]=0;
	for(l=2;l<n;n++){
		for(i=1;i<n-l+1;i++){
			j=i+l-1;
			m[i][j]=inf;
			for(k=i;k<j-1;k++){
				q=m[i][k]+m[k+1][j]+p[i-1]*p[k]*p[j];
				if( q<m[i][j] ){
					m[i][j]=q;
					s[i][j]=k;
				}
			}
		}
	}
	
	printf("K - Matrix \n");
	for(i=1;i<n;i++){
		for(j=2;j<=n;j++){
			printf("%d ",m[i][j]);
		}
	}
}


void main(){

	int i,size;
	printf("Enter the Size of Sequence:-");
	scanf("%d",&size);
	int p[size];
	printf("Enter the Sequence :-\n");
	for(i=0;i<size;i++)
		scanf("%d",&p[i]);
	
	matrix_chain(p,size);

}
